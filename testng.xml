<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Example Suite">
    <test name="Login Tests">
        <groups>
            <run>
                <include name="login success"/>
                <include name="login failed"/>
                <include name="login failed"/>
                <include name="login failed"/>
            </run>
        </groups>
        <classes>
            <class name="tests.TestSuiteExample"/>
        </classes>
    </test>
    <test name="Positive Tests">
        <groups>
            <run>
                <include name="search"/>
            </run>
        </groups>
        <classes>
            <class name="tests.TestSuiteExample"/>
        </classes>
    </test>
</suite>